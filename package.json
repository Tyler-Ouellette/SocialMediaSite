        "dotenv": "4.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.2.0",
        "gravatar": "^1.8.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.7.7",
        "request": "^2.88.0"
    },
    "devDependencies": {
        "concurrently": "^5.0.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.8.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
