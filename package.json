{
    "name": "devconnector",
    "version": "1.0.0",
    "description": "Social Network for developers",
    "main": "server.js",
    "now": {
        "alias": "tylerouellette.tech"
    },
    "scripts": {
        "start": "node server.js",
        "build": "npm run start && npm start --prefix client",
        "server": "nodemon server",
